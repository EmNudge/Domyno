{
  "compilerOptions": {
    "target": "es6",
    "module": "esNext",
    "lib": [
      "es6",
      "dom"
    ],
    "rootDir": "/",
    "types": [
      "jest"
    ],
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "declaration": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
